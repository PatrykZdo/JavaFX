               </children>
            </HBox>
            <HBox alignment="CENTER" layoutY="305.0" prefHeight="94.0" prefWidth="365.0" spacing="55">
               <children>
                  <Slider fx:id="glosnosc" prefHeight="18.0" prefWidth="140.0" showTickLabels="true" value="80.0" />
